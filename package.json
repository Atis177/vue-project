  "dependencies": {
    "express": "^4.16.2",
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.3",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.3",
    "@vue/cli-service": "^3.0.0-beta.3",
    "axios": "^0.18.0",
    "bootstrap": "^4.0.0",
    "json-server": "^0.12.1",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
